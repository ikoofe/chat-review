{
  "name": "@koofe/chat-review",
  "private": false,
  "homepage": "",
  "keywords": [],
  "type": "module",
 "version": "0.0.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "chat-review": "./dist/cli.mjs"
  },
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --write --cache .",
    "lint": "eslint --cache .",
    "typecheck": "pnpm -r --parallel run typecheck",
    "dev": "pnpm -r --parallel --filter='./packages/*' run dev",
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^18.14.0",
    "eslint": "^8.34.0",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.6.3",
    "unbuild": "1.1.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "packages/**/*.ts": [
      "eslint --cache --fix"
    ],
    "packages/**/*.d.ts": [
      "eslint --cache --fix"
    ]
  },
  "eslintConfig": {
    "rules": {
      "import/default": 0
    }
  },
  "packageManager": "pnpm@7.26.2",
  "pnpm": {
    "overrides": {}
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cac": "^6.7.14",
    "camelcase": "^7.0.1",
    "chatgpt": "^5.1.2",
    "consola": "^2.15.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
